{
  "expo": {
    "name": "FlashFits",
    "slug": "FlashFits",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/fevicon.png",
    "scheme": "flashfits",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.flashfits.app",
      "supportsTablet": true
    },
    "android": {
      "package": "com.flashfits.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/fevicon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/fevicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/fevicon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-secure-store"
    ],
    "splash": {
      "image": "./assets/images/fevicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "extra": {
      "BACKEND_URL": "https://ff-api-web.onrender.com",
      "eas": {
        "projectId": "5ffa6730-d676-4b63-a03d-df1db41ca12b"
      }
    },
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "owner": "efron"
  }
}
